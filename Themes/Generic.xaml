<ResourceDictionary xmlns:dxg="http://schemas.devexpress.com/winfx/2008/xaml/grid"  
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:dxc="http://schemas.devexpress.com/winfx/2008/xaml/charts"
                    xmlns:ssw="clr-namespace:DiagramDesigner.Windows.SuspensionWD"
                    xmlns:dxcore="http://schemas.devexpress.com/winfx/2008/xaml/core"
                    xmlns:i="clr-namespace:System.Windows.Interactivity;assembly=System.Windows.Interactivity"
                    xmlns:dxdo="http://schemas.devexpress.com/winfx/2008/xaml/docking"
                    xmlns:ed="http://schemas.microsoft.com/expression/2010/drawing"
                    xmlns:vm="clr-namespace:DiagramDesigner.ViewModel"
                    xmlns:btn="clr-namespace:DiagramDesigner.CustomControls.Buttons"
                    xmlns:lbl="clr-namespace:DiagramDesigner.CustomControls.Labels"
                    xmlns:chart="clr-namespace:DiagramDesigner.CustomControls.Charts"
                    xmlns:img="clr-namespace:DiagramDesigner.CustomControls.Images"
                    xmlns:list="clr-namespace:DiagramDesigner.CustomControls.Lists"
                    xmlns:wpf="clr-namespace:CefSharp.Wpf;assembly=CefSharp.Wpf"
                    xmlns:cef="clr-namespace:CefSharp;assembly=CefSharp.Core"
                    xmlns:lgd="clr-namespace:DiagramDesigner.CustomControls.Legends"
                    xmlns:brs="clr-namespace:DiagramDesigner.CustomControls.Browsers"
                    xmlns:cvt="clr-namespace:DiagramDesigner.Converters"
                    xmlns:local="clr-namespace:DiagramDesigner.CustomControls">

    <Storyboard x:Key="Storyboard">
        <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="EndAngle" Storyboard.TargetName="TargetCircle">
            <EasingDoubleKeyFrame KeyTime="0:0:4" Value="360"></EasingDoubleKeyFrame>
        </DoubleAnimationUsingKeyFrames>
    </Storyboard>
    <!--转换器-->
    <cvt:PercentToAngleConverter x:Key="PercentToAngleConverter"></cvt:PercentToAngleConverter>

    <!--测试控件-->
