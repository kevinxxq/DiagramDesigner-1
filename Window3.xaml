<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DiagramDesigner"
        xmlns:dxc="http://schemas.devexpress.com/winfx/2008/xaml/charts"
        xmlns:cs="clr-namespace:DiagramDesigner.CustomControls"
        xmlns:dxg="http://schemas.devexpress.com/winfx/2008/xaml/grid"
        xmlns:dxcore="http://schemas.devexpress.com/winfx/2008/xaml/core"
        xmlns:dxdo="http://schemas.devexpress.com/winfx/2008/xaml/docking"
        xmlns:ssw="clr-namespace:DiagramDesigner.Windows.SuspensionWD"    
        xmlns:system="clr-namespace:System;assembly=mscorlib"
        xmlns:model="clr-namespace:DiagramDesigner.Models"
        xmlns:vm="clr-namespace:DiagramDesigner.ViewModel"
        xmlns:list="clr-namespace:DiagramDesigner.CustomControls.Lists"
        xmlns:dxprg="http://schemas.devexpress.com/winfx/2008/xaml/propertygrid"
        xmlns:chart="clr-namespace:DiagramDesigner.CustomControls.Charts"
        xmlns:ed="http://schemas.microsoft.com/expression/2010/drawing"
        x:Class="DiagramDesigner.Window3"
        mc:Ignorable="d"
        Title="SplineAreaChart" Height="600" Width="700">
    <Window.Resources>
        <ResourceDictionary>
            <Storyboard x:Key="Storyboard">
                <!--<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="EndAngle" Storyboard.TargetName="TargetCircle">
                    <EasingDoubleKeyFrame KeyTime="0:0:4" Value="-360"></EasingDoubleKeyFrame>
                </DoubleAnimationUsingKeyFrames>-->
                <DoubleAnimation Storyboard.TargetName="TargetCircle" Storyboard.TargetProperty="EndAngle" To="360" Duration="0:0:1"></DoubleAnimation>
            </Storyboard>
        </ResourceDictionary>
    </Window.Resources>
    <Window.Triggers>
        <EventTrigger RoutedEvent="FrameworkElement.Loaded">
            <BeginStoryboard Storyboard="{StaticResource Storyboard}"></BeginStoryboard>
        </EventTrigger>
    </Window.Triggers>
    <Grid>
        <chart:CircleNumberChart></chart:CircleNumberChart>
    </Grid>
</Window>
